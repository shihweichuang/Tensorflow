   "source": [
    "import tensorflow as tf\n",
    "from tensorflow.keras import layers\n",
    "\n",
    "# 創建狀態向量長度為 5 的 SimpleRNN 層\n",
    "layer = layers.SimpleRNN(5)\n",
    "# 創建輸入的資料 (batch_size,timesteps,word_v)\n",
    "data = tf.random.normal([2, 10, 20])\n",
    "# 完成一次前向運算\n",
    "out = layer(data)\n",
    "print(out.shape)"
   ]
  }
